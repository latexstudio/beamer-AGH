\mode<presentation>
\definecolor{AGH@black}{RGB}{35,31,32}
\definecolor{AGH@red}{RGB}{176,16,40}
\definecolor{AGH@green}{RGB}{0,107,59} 
\setbeamercolor{palette primary}{fg=white,bg=AGH@red}
\setbeamercolor{palette secondary}{fg=white,bg=AGH@black}
\setbeamercolor{palette tertiary}{fg=white,bg=AGH@green}
\setbeamercolor{palette quaternary}{bg=white,fg=AGH@black}
\setbeamercolor{structure}{fg=AGH@green,bg=AGH@black}
\setbeamercolor{alerted text}{fg=AGH@black!20!AGH@red,bg=bg}
\setbeamercolor{example text}{fg=AGH@black!20!AGH@green,bg=bg}
\setbeamercolor{titlelike}{parent=palette secondary}
\setbeamercolor{block title}{fg=white,bg=AGH@black!30!bg}
\setbeamercolor{block title alerted}{fg=alerted text.fg,bg=alerted text.fg!30!bg}
\setbeamercolor{block title example}{fg=example text.fg,bg=example text.fg!30!bg}
\setbeamercolor{item}{fg=AGH@green}
\setbeamercolor{bibliography entry author}{fg=AGH@green,bg=blue!90!AGH@black}
\setbeamercolor{section in toc}{fg=AGH@black,bg=gray}
\setbeamercolor{background canvas}{bg=white}
\setbeamercolor{palette sidebar secondary}{fg=yellow,bg=blue}
\setbeamercolor{navigation symbols}{fg=AGH@black, bg=AGH@green}
\mode<all>
